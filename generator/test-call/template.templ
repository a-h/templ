{% package testcall %}

{% templ personTemplate(p person) %}
	<div>
		<h1>{%= p.name %}</h1>
		<div style="font-family: &#39;sans-serif&#39;" id="test" data-contents={%= `something with "quotes" and a <tag>` %}>
			{%! email(p.email) %}
		</div>
	</div>
{% endtempl %}

{% templ email(s string) %}
	<div>{%= "email:" %}<a href={%= templ.URL("mailto: " + s) %}>{%= s %}</a></div>
{% endtempl %}

